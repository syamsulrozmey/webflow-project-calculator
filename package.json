    "clsx": "^2.1.1",
    "ioredis": "^5.8.2",
    "lucide-react": "^0.473.0",
    "next": "16.0.3",
    "pdf-lib": "^1.17.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^2.5.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@jest/test-sequencer": "^29.7.0",
    "@types/jest": "^29.5.14",
