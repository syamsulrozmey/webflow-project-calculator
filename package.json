    "dotenv-cli": "^7.4.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "jest": "^29.7.0",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma": "^6.19.0",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5"
