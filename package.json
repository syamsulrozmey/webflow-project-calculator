{
  "name": "webflow-website-calculator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "stack:up": "docker compose up -d",
